{
	"schema_version": 1,
	"quilt_loader": {
		"group": "mun",
		"id": "chat_lib",
		"version": "${version}",
		"metadata": {
			"name": "Chat Lib",
			"description": "A library that makes checking what people say easier.",
			"license": "MIT",
			"contributors": {
				"Munsterlandr": "Owner"
			},
			"contact": {
				"issues": "https://github.com/Munsterlandr/chat-lib/issues",
				"sources": "https://github.com/Munsterlandr/chat-lib"
			},
			"icon": "assets/chat_lib/icon.png"
		},
		"intermediate_mappings": "net.fabricmc:intermediary",
		"entrypoints": {
			"init": {
				"adapter": "kotlin",
				"value": "mun.chat_lib.ChatLib"
			}
		},
		"depends": [
			{
				"id": "quilt_loader",
				"versions": ">=0.17.0-"
			},
			{
				"id": "quilted_fabric_api",
				"versions": ">=4.0.0-"
			},
			{
				"id": "qkl",
				"versions": ">=0.1.4-"
			},
			{
				"id": "minecraft",
				"versions": ">=1.19.2"
			}
		]
	},
	"mixin": "chat_lib.mixins.json"
}
